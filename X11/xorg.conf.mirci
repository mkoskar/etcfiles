# vim: ft=xf86conf

Section "Device"
    Identifier "card0"
    Driver "intel"
    BusID "PCI:0:2:0"

    #Option "AccelMethod" "uxa"
    Option "TearFree" "on"
    #Option "ReprobeOutputs" "on"
    #Option "XvPreferOverlay" "on"
    #Option "Vsync" "off"
    #Option "PageFlip" "off"
    #Option "SwapbuffersWait" "off"
    #Option "TripleBuffer" "off"
EndSection
